{
  "name": "jsonuri",
  "version": "2.1.11",
  "description": "Use URI path to get or set data",
  "keywords": [
    "array",
    "data",
    "data operator",
    "javascript",
    "json",
    "jsonuri",
    "nodejs",
    "object",
    "path",
    "uri",
    "vue"
  ],
  "homepage": "https://jsonuri.js.org",
  "bugs": "https://github.com/allgay/jsonuri/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/allgay/jsonuri.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run lint && node scripts/build.js",
    "clean": "rimraf ./dist",
    "debug": "node --inspect-brk dist/index.js",
    "deploy:gh-pages": "node scripts/gh-pages.js",
    "dev": "m-sh 'npm run dev:tsc'",
    "dev:tsc": "npm run clean && tsc -w",
    "jasmine": "istanbul cover jasmine-node test",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepublishOnly": "npm test && node scripts/pre-publish.js",
    "test": "npm run lint && npm run build && npm run jasmine"
  },
  "config": {
    "ghooks": {
      "pre-commit": "sh scripts/pre-commit.sh"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.7.12",
    "gh-pages": "^2.1.1",
    "ghooks": "^2.0.4",
    "imod": "^1.1.4",
    "istanbul": "^0.4.5",
    "jasmine-node": "^3.0.0",
    "multi-shell": "^1.1.0",
    "rimraf": "*",
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=6"
  }
}
