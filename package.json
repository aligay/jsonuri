    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "test": "pnpm run coverage",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/serialize-javascript": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "husky": "^9.1.7",
    "immer": "^10.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
